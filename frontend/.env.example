# HepatoCAI Frontend Environment Variables - Example Configuration
# Copy this file to .env and fill in the appropriate values for your environment

# ======================================
# APPLICATION CONFIGURATION
# ======================================
VITE_APP_NAME=HepatoCAI
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
VITE_DEBUG=true
VITE_LOG_LEVEL=info

# ======================================
# API CONFIGURATION
# ======================================
# Backend API base URL - change for production
VITE_API_BASE_URL=http://127.0.0.1:8000
# Frontend URL for CORS and OAuth redirects
VITE_FRONTEND_URL=http://localhost:5173
# API request timeout in milliseconds
VITE_API_TIMEOUT=10000
# API version
VITE_API_VERSION=v1
# Number of retry attempts for failed API calls
VITE_API_RETRY_ATTEMPTS=3
# Delay between retry attempts in milliseconds
VITE_API_RETRY_DELAY=1000

# ======================================
# DEVELOPMENT SERVER CONFIGURATION
# ======================================
# Development server port
VITE_DEV_SERVER_PORT=5173
# Allow external connections (true/false)
VITE_DEV_SERVER_HOST=true
# Auto-open browser on start (true/false)
VITE_DEV_SERVER_OPEN=false
# Enable HTTPS for development (true/false)
VITE_DEV_SERVER_HTTPS=false
# Fail if port is already in use (true/false)
VITE_DEV_SERVER_STRICT_PORT=false

# ======================================
# PREVIEW SERVER CONFIGURATION
# ======================================
# Preview server port (for production preview)
VITE_PREVIEW_PORT=4173
# Allow external connections for preview (true/false)
VITE_PREVIEW_HOST=true
# Enable HTTPS for preview (true/false)
VITE_PREVIEW_HTTPS=false

# ======================================
# BUILD CONFIGURATION
# ======================================
# Build output directory
VITE_BUILD_OUTPUT_DIR=dist
# Assets directory within build output
VITE_BUILD_ASSETS_DIR=assets
# Generate source maps (true/false)
VITE_BUILD_SOURCEMAP=false
# Minification method (esbuild/terser/false)
VITE_BUILD_MINIFY=esbuild
# Build target (comma-separated: es2015,es2020)
VITE_BUILD_TARGET=es2015
# Chunk size warning limit in KB
VITE_BUILD_CHUNK_SIZE_WARNING=500
# Base path for deployment (/ for root, /app/ for subdirectory)
VITE_BASE_PATH=/

# ======================================
# OPTIMIZATION CONFIGURATION
# ======================================
# Dependencies to exclude from optimization (comma-separated)
VITE_OPTIMIZE_DEPS_EXCLUDE=
# Additional SCSS data to prepend to every SCSS file
VITE_SCSS_ADDITIONAL_DATA=
# Clear terminal screen on file changes (true/false)
VITE_CLEAR_SCREEN=false

# ======================================
# OAUTH CONFIGURATION
# ======================================
# Google OAuth Client ID - get from Google Cloud Console
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# ======================================
# FEATURE FLAGS
# ======================================
# Enable debug console in production (true/false)
VITE_ENABLE_DEBUG_CONSOLE=false
# Enable diagnosis debugging features (true/false)
VITE_ENABLE_DIAGNOSIS_DEBUG=false
# Enable analytics tracking (true/false)
VITE_ENABLE_ANALYTICS=true
# Enable service worker for offline support (true/false)
VITE_ENABLE_SERVICE_WORKER=false
# Enable push notifications (true/false)
VITE_ENABLE_NOTIFICATIONS=true
# Enable offline mode functionality (true/false)
VITE_ENABLE_OFFLINE_MODE=false

# ======================================
# PERFORMANCE SETTINGS
# ======================================
# Enable response caching (true/false)
VITE_CACHE_ENABLED=true
# Cache time-to-live in milliseconds
VITE_CACHE_TTL=300000
# Input debounce delay in milliseconds
VITE_DEBOUNCE_DELAY=300
# Scroll/resize throttle delay in milliseconds
VITE_THROTTLE_DELAY=100
# Enable lazy loading of components (true/false)
VITE_LAZY_LOADING=true
# Enable image optimization (true/false)
VITE_IMAGE_OPTIMIZATION=true
# Enable performance monitoring (true/false)
VITE_PERFORMANCE_MONITORING=false
# Enable bundle analyzer in build (true/false)
VITE_BUNDLE_ANALYZER=false

# ======================================
# SECURITY CONFIGURATION
# ======================================
# Enable Content Security Policy (true/false)
VITE_ENABLE_CSP=true
# CSP report URI for violation reports
VITE_CSP_REPORT_URI=
# Enable HTTP Strict Transport Security (true/false)
VITE_ENABLE_HSTS=false
# Enable security headers (true/false)
VITE_SECURITY_HEADERS=true

# ======================================
# SEO & META CONFIGURATION
# ======================================
# Application title for browser tab and SEO
VITE_APP_TITLE=HepatoC AI - Advanced Liver Health Diagnosis
# Meta description for SEO
VITE_APP_DESCRIPTION=Advanced AI-powered hepatitis diagnosis and liver health monitoring platform
# Meta keywords for SEO (comma-separated)
VITE_APP_KEYWORDS=hepatitis,liver health,AI diagnosis,medical,healthcare
# Application author
VITE_APP_AUTHOR=HepatoCAI Team
# Theme color for mobile browsers
VITE_APP_THEME_COLOR=#2563EB
# Background color for PWA splash screen
VITE_APP_BACKGROUND_COLOR=#ffffff

# ======================================
# ANALYTICS & MONITORING
# ======================================
# Google Analytics Measurement ID (optional)
VITE_GOOGLE_ANALYTICS_ID=
# Sentry DSN for error tracking (optional)
VITE_SENTRY_DSN=
# Hotjar Site ID for user behavior analytics (optional)
VITE_HOTJAR_ID=
# Mixpanel Project Token (optional)
VITE_MIXPANEL_TOKEN=

# ======================================
# CDN & EXTERNAL SERVICES
# ======================================
# CDN URL for static assets (optional)
VITE_CDN_URL=
# Image CDN URL for optimized images (optional)
VITE_IMAGE_CDN_URL=
# Static assets URL (optional, different from CDN)
VITE_STATIC_ASSETS_URL=

# ======================================
# DEPLOYMENT CONFIGURATION
# ======================================
# Current deployment environment
VITE_DEPLOYMENT_ENVIRONMENT=development
# Deployment version/tag
VITE_DEPLOYMENT_VERSION=
# Build timestamp (auto-generated in CI/CD)
VITE_BUILD_TIMESTAMP=
# Git commit hash (auto-generated in CI/CD)
VITE_GIT_COMMIT_HASH=
# CI/CD build number (auto-generated)
VITE_CI_BUILD_NUMBER=
